{
  "name": "api",
  "version": "1.0.0",
  "description": "REST API built with Node.js, TypeScript, and Express",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/server.js",
    "dev": "tsx watch --env-file=.env src/server.ts",
    "db:push": "node  --env-file=$ENV_FILE --import tsx ./node_modules/drizzle-kit/bin.cjs push",
    "db:studio": "node --import tsx --env-file=.env ./node_modules/drizzle-kit/bin.cjs studio",
    "lint": "eslint ./src/**/* --fix && prettier --write ./src/**/*",
    "test": "NODE_ENV=test node --import tsx --env-file=.env --test ./src/**/*.test.ts",
    "test:watch": "NODE_ENV=test node --import tsx --env-file=.env --test --watch ./src/**/*.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^9.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "lucia": "^3.2.0",
    "oslo": "^1.2.1",
    "pino": "^9.4.0",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": "v22.6.0"
  },
  "packageManager": "pnpm@9.9.0"
}
