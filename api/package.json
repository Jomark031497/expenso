{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/server.js",
    "dev": "NODE_ENV=development tsx watch --env-file=.env src/server.ts",
    "lint": "eslint ./src/**/*.{ts,js}",
    "format": "pnpm prettier --write ./src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "drizzle-kit": "node --import tsx ./node_modules/drizzle-kit/bin.cjs",
    "test": "NODE_ENV=test node --import tsx --env-file=.env --test --experimental-test-coverage ./src/**/*.test.ts",
    "test:watch": "NODE_ENV=test node --import tsx --env-file=.env --test --watch ./src/**/*.test.ts"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "arctic": "^1.9.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "oslo": "^1.2.1",
    "pino": "^9.4.0",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@faker-js/faker": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.6.1",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
